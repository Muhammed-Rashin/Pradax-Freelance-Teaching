    <script>
        const formsss = document.getElementById("signup-form");
        const usernameText = document.getElementById("username-text");
        const textField = document.getElementById("username");
        const subButton = document.querySelector("#signup-btn");




        function doLogin() {
            let username = document.getElementById("login-username").value
            let password = document.getElementById("login-password").value

            jQuery.ajax({
                url: "/login",

                data: {
                    username: username,
                    password: password,
                },

                type: 'POST',
                success: function (data) {
                    console.log(data);
                    if (data == "Success") {
                        window.location.href = "/student";
                    }
                    else{
                        document.getElementById("incorrectAlert").removeAttribute("hidden");
                    }
                },
                error: function () {
                    console.log('error')
                }
            })

        }



        function isUserNameExist() {

            let username = $("#username").val()
            if (username.length > 0) {
                jQuery.ajax({
                    url: "/signup/isUserNameExist",
                    data: 'username=' + $("#username").val(),
                    type: 'POST',
                    success: function (data) {
                        if (data.status) {
                            usernameText.style.color = 'green';
                            textField.style.marginTop = '-2%';
                            usernameText.innerHTML = 'Username available.'
                            subButton.removeAttribute("disabled");
                        }
                        else {
                            usernameText.style.color = 'red';
                            usernameText.innerHTML = 'Username already taken.'
                            subButton.setAttribute('disabled', '');

                        }
                    },
                    error: function () {
                        console.log('error')
                    }
                })
            }
            else {
                usernameText.innerHTML = ''
            }
        } 
    </script>